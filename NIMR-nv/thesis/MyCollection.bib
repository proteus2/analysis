@article{Kim2013b,
abstract = {A convective gravity-wave parameterization (GWP) is introduced in the Met Office Unified Model (MetUM) in addition to the existing GWPs, and the tropical quasi-biennial oscillation (QBO) is simulated. We replace a significant amount of momentum flux, which is originally launched by the background GWP, with the convective gravity-wave (CGW) momentum flux with a broad wave spectrum that is explicitly determined by the convective sources. Compared to the experiment without the CGW parameterization, including the CGW parameterization results in a greater variability in the duration of each phase of the QBO,which is closer to observed. Including the CGWs also strengthens the annual cycle of zonal wind and it allows for stronger modulation of the semi-annual oscillation.},
author = {Kim, Young-Ha and Bushell, A. C. and Jackson, D. R. and Chun, Hye-Yeong},
doi = {10.1002/grl.50353},
file = {:D$\backslash$:/cloud/DaumCloud/논문/2013/Kim et al.\_2013\_Geophys. Res. Lett.pdf:pdf},
issn = {00948276},
journal = {Geophys. Res. Lett.},
month = mar,
number = {9},
pages = {1873--1877},
title = {{Impacts of introducing a convective gravity-wave parameterization upon the QBO in the Met Office Unified Model}},
url = {http://doi.wiley.com/10.1002/grl.50353},
volume = {40},
year = {2013}
}
@phdthesis{Kim2008,
abstract = {The characteristics and origins of the observed mesospheric gravity waves in Korean Peninsula were investigated, and the propagation of the convective gravity waves in the mesosphere and characteristics of the secondary waves were examined by using a 2-D numerical model. From the airglow images of Korean Peninsula, it was observed that the mesospheric gravity waves had characteristics of having short horizontal wavelengths and periods ( $\lambda$h = 10 \~{} 40 km, $\tau$ = 5 \~{} 35 min) and widely distributed phase speeds (0 \~{} 70 m s-1). Based on the observed wave characteristics, ray-tracing was conducted to find the trajectories of gravity waves. In the ray-tracing calculations, 69 percent of the cases were terminated in the mesosphere, whereas 31 percent of the cases reached the troposphere. For the cases which reached the troposphere, their sources were investigated and convection was considered as the most probable source of the observed gravity waves. The importance of the mesospheric sources was noted from the fact that back-trajectory calculations were terminated in the mesosphere for many cases. It was observed that the most of the cases terminated in the mesosphere had shorter horizontal wavelengths than those terminated in the troposphere. Gravity waves forced by convection were simulated using the 2-D cloud-resolving model. breaking and secondary waves generated by the breaking were The aspects of their investigated for the background flows of four seasons. Although the structures of mesospheric gravity waves and the aspects of their breaking were different among four seasons, there were also some similarities as follows: The wave-breaking structure followed the phase of gravity waves propagating into the mesosphere and it became more complex as time passed. The secondary waves having broad spectra were observed outside the wave-breaking region and they propagated upward and/or downward if circumstance allowed. In addition, momentum flux and drag forcing were analyzed to see the effects of secondary waves quantitatively. As a result, secondary waves cancelled more than \~{}1/3 of the momentum flux and drag forcing by convective gravity waves in the middle atmosphere. Finally, the relation between the primary and secondary waves was investigated through some simple experiments. The results showed that the characteristics of the secondary waves in various basic states were different from each other, due mainly to the difference in wave propagation condition rather than in wave sources.},
author = {Kim, Young-Ha},
file = {:D$\backslash$:/cloud/DaumCloud/논문/2008/Kim\_2008\_Unknown.pdf:pdf},
keywords = {airglow,mesospheric gravity waves,ray-tracing,secondary wave,wave-breaking},
pages = {1--121},
school = {Yonsei University},
title = {한반도 중간권 대기 중력파의 특성과 2차 중력파의 영향},
year = {2008}
}
