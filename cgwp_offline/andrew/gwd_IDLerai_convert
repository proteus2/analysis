;
; Now link to fort.17 and run gw_ussp_offline.exe ...

;
; Read in flux output generated by offline Fortran program text output
indir='$HOME/Model/um10p8'
stime='1993-05-01'
; Define full 3D fields for input to offline scheme
n3siz = [ndsiz[0], 1, ndsiz[1]]
;
idir=2
cosphi=[1,-1]
fpsiz=lonarr(4)
fpsiz[0:2]=n3siz
fpsiz[3]=idir
filenuin=indir+'/era-interim_eq_'+stime+'_u.dat'
;
gw_off_calc_readat,filenuin,insizarr=ndsiz,datarro=uonp,dimarro=udims
istart = 0  & iend = n3siz[0]-1
jstart = 0  & jend = n3siz[1]-1
kstart = 0  & kend = n3siz[2]-1
nplevs=n3siz[2]
;
filename=indir+'/fptot_UMGA7_P0_'+stime+'.txt'
offdata=READ_ASCII(filename, DATA_START=17)
noffdata=size(offdata.FIELD1,/N_elements)
fptot=reform(reform(offdata.FIELD1,noffdata),fpsiz)
;
mfptot0=fltarr(nplevs)
for k=kstart,kend do mfptot0[k]=max(1000.*fptot[istart:iend,jstart:jend,k,0])
llaunch=min(where(mfptot0 gt 0))
print,llaunch
;
accgwphi=fltarr(fpsiz)
gwaccew=fltarr(fpsiz[0:2])
p_standard=100000.
zpro=reform(udims[kstart:kend,1],[1,1,nplevs])
ppro=p_standard/exp(zpro/7.)
;
for k=llaunch+1,kend do begin  & $
  for j=jstart,jend do begin  & $
    for i=istart,iend do begin  & $
      for jdir=0,idir-1 do begin  & $
        accgwphi[i,j,k,jdir] = (fptot[i,j,k,jdir] - fptot[i,j,k-1,jdir]) *  $
                                  g_earth / (ppro[k] - ppro[k-1]) & $
        gwaccew[i,j,k] = gwaccew[i,j,k] + (accgwphi[i,j,k,jdir]*cosphi[jdir]) & $
      endfor  & $
    endfor  & $
  endfor  & $
endfor
;
zmuonp = reform(rebin(uonp,1,fpsiz[2]))
zmgwaccew = reform(rebin(reform(gwaccew),1,fpsiz[2]))
zmfptot   = reform(rebin(reform(fptot),[1,fpsiz[2:3]]))
;
.run Snapplot
;
taga = 'ZmU ERAi Eq ('+stime+')'
tagb = 'Calculated Flux'
; tagb= 'Flux scaled: 1mPa at 100hPa'
fspd = 86400.
; xlimu=[-32.,28.]
xlimu=[-40.,20.]
apro = zmuonp[kstart:kend]
bpro = fspd*5.*zmgwaccew[kstart:kend]
cpro = 1000.*zmfptot[kstart:kend,0]
dpro = 1000.*zmfptot[kstart:kend,1]
;
pr,d=printer,/ps,/landscape,/col
!p.multi=[0,2,1]
restore_colours,'./pcsinb'
refcol=[0.75,0.375,0.0625,0.5,0.4375,0.3125]
icol=refcol*!d.table_size
snapplot,apro,zpro,tit=taga,ylim=[0.,64.],xlim=xlimu,$
ord='Model level height (km)',abs='U (m / s)',xref=0.
oplot,[apro[llaunch],apro[llaunch]],zpro[[kstart,kend]],linestyle=2,thick=3.,YType=0
oplot,apro,zpro,linestyle=0,thick=3.,YType=0
oplot,bpro,zpro,linestyle=0,col=icol[0],Psym=1,symsiz=0.8,thick=3.,YType=0
oplot,bpro,zpro,linestyle=0,col=icol[0],thick=3.,YType=0
xyouts,xlimu[0]+2.,60.,'Net Eastward Accel x5',col=icol[0],charsize=1.2,charthick=6.
;
xlim=[-0.1,1.7]  & dx=0.7
; xlim=[-0.5,11.5]  & dx=6
snapplot,cpro,zpro,tit=tagb,ylim=[0.,64.],xlim=xlim,$
ord='Model level height (km)',abs='GW flux (mPa)',xref=0.0
oplot,cpro,zpro,linestyle=0,thick=3.,YType=0
oplot,dpro,zpro,linestyle=0,col=icol[1],Psym=7,symsiz=0.8,thick=3.,YType=0
oplot,dpro,zpro,linestyle=0,col=icol[1],thick=3.,YType=0
xyouts,xlim[1]-dx,60.,'Eastward',col=0,charsize=1.2,charthick=6.
xyouts,xlim[1]-dx,56.,'Westward',col=icol[1],charsize=1.2,charthick=6.
;
prend,printer,/view,/keep
;
; print,'; UMGA7 scaled to 10mPa at 100hPa launch height '+stime
print,'; UMGA7 Standard settings '+stime
starrwriter,bpro,'EqZm EW accel'
starrwriter,cpro,'EqZm Eastward flux'
starrwriter,dpro,'EqZm Westward flux'
;
